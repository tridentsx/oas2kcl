{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "https://example.com/schemas/ethernet.json",
	"type": "object",
	"properties": {
		"name": {
			"type": "string"
		},
		"socket": {
			"type": "object",
			"additionalProperties": {
				"type": "integer"
			},
			"default": {
